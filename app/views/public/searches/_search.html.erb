<% if member_signed_in? %>
  <div class="search_form">
    <%= form_with url: search_path, local: true, method: :get do |f| %>
      <%= f.text_field :word %>
      <%= f.select :range, options_for_select([['投稿'], ['会員'],['場所']]) %>
      <button type="submit">
        <i class="fas fa-search"></i>
      </button>
    <% end %>
  </div>
<% end %>