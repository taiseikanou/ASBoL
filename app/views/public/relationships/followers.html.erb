<% if @members.exists? %>
<div class="container px-5 py-4">
  <div class="row align-items-center justify-content-between text-light">
    <h5 class="col-3">会員一覧</h4>
  </div>
  <table class="table table-hover table-inverse cart_products text-light">
    <tr>
      <th class="table-Default">ユーザー名</th>
      <th class="table-Default"></th></th>
    </tr>
    <% @members.each do |member| %>
      <tr >
        <td><%= member.name %></td>
        <td>
          <%= link_to member_path(member.id) do %>
          投稿一覧
          <% end %>
        </td>
    　</tr>
    <% end %>
  </table>
</div>
<% else %>
  <h1 class = "text-center text-light" style= "margin-top:200px;">ユーザーがいません</h1>
<% end %>
